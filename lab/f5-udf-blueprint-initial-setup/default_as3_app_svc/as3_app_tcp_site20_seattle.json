{
  "class": "AS3",
  "action": "deploy",
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.16.0",
    "target": {
      "address": "10.1.1.7"
    },
    "backend": {
      "class": "Tenant",
      "site20tcp": {
        "class": "Application",
        "schemaOverlay": "AS3-F5-FastL4-TCP-lb-template-default-v2",
        "template": "l4",
        "serviceMain": {
          "pool": "Pool",
          "enable": true,
          "profileL4": {
            "use": "L4_Profile"
          },
          "virtualPort": 80,
          "virtualAddresses": [
            "10.1.10.120"
          ],
          "profileAnalyticsTcp": {
            "use": "Analytics_TCP_Profile"
          },
          "class": "Service_L4"
        },
        "Pool": {
          "members": [
            {
              "adminState": "enable",
              "servicePort": 8080,
              "serverAddresses": [
                "10.1.20.116"
              ]
            },
            {
              "serverAddresses": [
                "10.1.20.116"
              ],
              "servicePort": 8080
            }
          ],
          "class": "Pool"
        },
        "L4_Profile": {
          "tcpCloseTimeout": 5,
          "tcpHandshakeTimeout": 5,
          "class": "L4_Profile"
        },
        "Analytics_TCP_Profile": {
          "collectCity": true,
          "collectRegion": true,
          "collectCountry": true,
          "collectPostCode": true,
          "collectContinent": true,
          "collectedByClientSide": true,
          "collectedByServerSide": true,
          "class": "Analytics_TCP_Profile"
        }
      }
    }
  }
}