{
  "class": "AS3",
  "action": "deploy",
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.12.0",
    "target": {
      "address": "10.1.1.8"
    },
    "security": {
      "class": "Tenant",
      "site16_boston": {
        "class": "Application",
        "schemaOverlay": "AS3-F5-HTTP-lb-traffic-capture-template-big-iq-default-v1",
        "template": "http",
        "serviceMain": {
          "class": "Service_HTTP",
          "virtualPort": 80,
          "profileAnalytics": {
            "use": "Analytics_Profile"
          },
          "profileHTTP": {
            "use": "HTTP_Profile"
          },
          "virtualAddresses": [
            "10.1.10.116"
          ],
          "pool": "Pool",
          "enable": true
        },
        "Analytics_Profile": {
          "class": "Analytics_Profile",
          "collectUserAgent": true,
          "collectClientSideStatistics": true,
          "collectGeo": true,
          "collectUrl": true,
          "collectPageLoadTime": true,
          "collectOsAndBrowser": true,
          "collectMethod": true,
          "collectResponseCode": true,
          "capturedTrafficInternalLogging": true,
          "captureFilter": {
            "requestCapturedParts": "all",
            "responseCapturedParts": "all"
          }
        },
        "HTTP_Profile": {
          "class": "HTTP_Profile",
          "fallbackRedirect": "https://www.example.com/404",
          "fallbackStatusCodes": [
            404
          ]
        },
        "Pool": {
          "class": "Pool",
          "members": [
            {
              "servicePort": 80,
              "adminState": "enable",
              "serverAddresses": [
                "10.1.20.110"
              ]
            },
            {
              "servicePort": 80,
              "serverAddresses": [
                "10.1.20.111"
              ]
            }
          ]
        }
      }
    }
  }
}